{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name":"api",
      "urls":[
        "https://toma5digital.herokuapp.com/api/"
      ],
      "cacheConfig":{
        "strategy":"performance",
        "maxAge":"1d",
        "maxSize":100,
        "timeout":"5s"
      }
    }
  ]
}
